{
    "name": "landingi/aws-bundle",
    "description": "Landingi Standard Library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.369"
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^13.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpdoc-parser": "^1.33.0",
        "phpstan/phpstan": "^2.1.14",
        "phpstan/phpstan-phpunit": "^2.0.3",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Landingi\\AwsBundle\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Landingi\\AwsBundle\\" : "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
