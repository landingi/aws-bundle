{
    "name": "landingi/aws-bundle",
    "description": "Landingi Standard Library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.369"
    },
    "require-dev": {
        "landingi/php-coding-standards": "1.0.3",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Landingi\\AwsBundle\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Landingi\\AwsBundle\\" : "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
